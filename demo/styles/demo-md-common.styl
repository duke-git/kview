.example-container
    font-family 'Avenir', Helvetica, Arial, sans-serif
    -webkit-font-smoothing antialiased
    -moz-osx-font-smoothing grayscale
    text-align left
    color #2c3e50
    padding 10px 25px
    overflow hidden
    h1
        font-size 20px
        margin-bottom 10px
    p
        font-size 14px
        margin-top 10px
        clear both
    h2
      margin 20px 0
    .example
        float left
        width 45%
        padding-top 20px
        text-align center
        button
            margin: 0
    code
        color #3F51B5
        font-size 14px
        border-radius 4px
        background-color #fff
    .api-table
        border 1px solid #e9e9e9
        width 100%
        margin 20px 0
        font-size Arial
        th
            border 1px solid #e9e9e9
            padding 8px 16px
            text-align left
            font-size 12px
        td
          border 1px solid #e9e9e9
          padding 8px 16px
          text-align left
          font-size 12px


